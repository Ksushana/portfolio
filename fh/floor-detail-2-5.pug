extends pug/layout.pug

block variables
  - let title = 'Искусные планировки';
  - let isInner = true;
  - let isFHPage = true;
  - let logoNoScroll = true;
  - let headerBackground = 'header__menu--fantastic';

block content
  .container.floor-detail-container.blue-burger-container
    include pug/phone-icon.pug
    h1.title.title--page.title--big искусные планировки

    .choice
      .swiper-container.choice-container
      .swiper-wrapper.tabs-links
        .swiper-slide
          button.switch-button.switch-button-active( type="button") план квартиры
        .swiper-slide
          button.switch-button( type="button") план этажа
        .swiper-slide
          button.switch-button( type="button") пример дизайна
    .tab.is-active
      section.flat-plan
        .flat-plan__col.flat-plan__col--left
          .flat-plan__desc Пентхаус № 2 224 м<sup>2</sup>
        .flat-plan__col.flat-plan__col--center
          .flat-plan__img
            a(href="#").popup-trigger
              .flat-plan-img
                p.flat-plan-img__text.flat-plan-img__text--left Малая Грузинская
                p.flat-plan-img__text.flat-plan-img__text--bottom Большой Тишинский переулок
                img(src="img/flat-plan-full@1x.png", alt="План квартиры", srcset="img/flat-plan-full@2x.png 2x")
        .flat-plan__col.flat-plan__col--right
          .flat-plan__details
            .flat-plan__compas.floor-compas
