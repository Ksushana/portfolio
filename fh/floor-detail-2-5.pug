        button(type="button", aria-label="Кнопка закрытия").flat-plan-popup__btn.popup-btn
        .flat-plan-popup__img
          .flat-plan-img
            p.flat-plan-img__text.flat-plan-img__text--left Малая Грузинская
            p.flat-plan-img__text.flat-plan-img__text--bottom Большой Тишинский переулок
            img(src="img/flat-plan-full@1x.png", alt="План квартиры", srcset="img/flat-plan-full@2x.png 2x")
    .tab
      section.floor.floor--1
        .floor__col.floor__col--left
          .floor__desc
            p.floor__number Этаж 1
            p.floor__text Возможно объединение квартир
        .floor__col.floor__col--center
          .floor__img
            p.floor__img-text.floor__img-text--left Малая Грузинская
            p.floor__img-text.floor__img-text--bottom Большой Тишинский переулок
            p.floor__img-text.floor__img-text--garden Сад
            a(href="floor-detail-2-5.html").floor__flat.floor__flat--1.floor__flat--active
              .floor__flat-number № 1<br>243 м<sup>2</sup>
            a(href="floor-detail-2-6.html").floor__flat.floor__flat--2
              .floor__flat-number № 2<br>224 м<sup>2</sup>
            a(href="floor-detail-2-7.html").floor__flat.floor__flat--3
              .floor__flat-number № 3<br>224 м<sup>2</sup>
            a(href="floor-detail-2-8.html").floor__flat.floor__flat--4
              .floor__flat-number № 4<br>224 м<sup>2</sup>
            //- a.floor__flat.floor__flat--sold
            //-   .floor__flat-number № 3<br>ПРОДАНО
            .floor__img-cover
              +flats-hover-1
              img(src='img/fh_floor_1.svg', alt='План этажа')
        .floor__col.floor__col--right
          .floor__compas
            span C
            svg(width='14', height='25', viewbox='0 0 14 25', fill='none', xmlns='http://www.w3.org/2000/svg')
              path(d='M4.57587 1.56719C4.33685 1.42891 4.03098 1.51057 3.8927 1.7496L1.63926 5.64472C1.50097 5.88375 1.58264 6.18961 1.82167 6.3279C2.06069 6.46618 2.36656 6.38451 2.50484 6.14549L4.5079 2.68315L7.97023 4.68621C8.20926 4.8245 8.51512 4.74283 8.65341 4.5038C8.79169 4.26478 8.71002 3.95891 8.471 3.82063L4.57587 1.56719ZM10.4838 23.1264L4.80857 1.871L3.84241 2.12896L9.51766 23.3844L10.4838 23.1264Z', fill='#10069F')
